{
  "manifest_version": 3,
  "name": "GitLab - PR Copy",
  "description": "Copies all the PRs visible on screen",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://gitlab.com/tekion/development/daet/map/tekion_apps/-/merge_requests*"],
      "js": ["content.js"]
    }
  ]
}
